@page
@model AdminPage_JagdeepSingh.Pages.Admin.Supplier.SupplierCreateModel
@{
    ViewData["Title"] = "Admin page";
}
<div class="create-form, border-white">
    <form method="post">
        <hidden asp-for="Shipper.ID" />
        <h1 class="font-weight-bold"> Supplier </h1>
        <div>
            <label asp-for="Supplier.companyName"></label>
            <div>
                <input asp-for="Supplier.companyName" />
                <span asp-validation-for="Supplier.companyName"></span>
            </div>
        </div>
        <div>
            <label asp-for="Supplier.contactName"></label>
            <div>
                <input asp-for="Supplier.contactName" />
                <span asp-validation-for="Supplier.contactName"></span>
            </div>
        </div>
        <div>
            <label asp-for="Supplier.Address"></label>
            <div>
                <input asp-for="Supplier.Address" />
                <span asp-validation-for="Supplier.Address"></span>
            </div>
        </div>
        <div>
            <label asp-for="Supplier.city"></label>
            <div>
                <input asp-for="Supplier.city" />
                <span asp-validation-for="Supplier.city"></span>
            </div>
        </div>
        <div>
            <label asp-for="Supplier.region"></label>
            <div>
                <input asp-for="Supplier.region" />
                <span asp-validation-for="Supplier.region"></span>
            </div>
        </div>
        <div>
            <label asp-for="Supplier.postalCode"></label>
            <div>
                <input asp-for="Supplier.postalCode" />
                <span asp-validation-for="Supplier.postalCode"></span>
            </div>
        </div>
        <div>
            <label asp-for="Supplier.country"></label>
            <div>
                <input asp-for="Supplier.country" />
                <span asp-validation-for="Supplier.country"></span>
            </div>
        </div>

        <div>
            <label asp-for="Supplier.phone"></label>

            <div>
                <input asp-for="Supplier.phone" />
                <span asp-validation-for="Supplier.phone"></span>
            </div>
        </div>
        <div>
            <label asp-for="Supplier.fax"></label>

            <div>
                <input asp-for="Supplier.fax" />
                <span asp-validation-for="Supplier.fax"></span>
            </div>
        </div>

        <div asp-validation-summary="All">
            <label class="card-title, font-weight-bold"> Error Summary</label>
        </div>
        <input type="submit" value="Create">
    </form>
</div>
@section scripts
{
    <script src="~/lib/jquery-validation/dist/additional-methods.min.js"></script>

    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>

    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
}